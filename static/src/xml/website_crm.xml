<?xml version="1.0" encoding="utf-8"?>
<odoo> 
    <template id="contactus_form" name="Contact Form" inherit_id="website_crm.contactus_form" >
        <xpath expr="//form/div[last()]" position="before">
            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Place_of_birth">Place of birth</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="Place_of_birth" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="family_situation">Family situation</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <select class="form-control o_website_form_input" name="family_situation" id="family_situation">
                        <option></option>
                        <option value="single">Single</option>
                        <option value="married">Married</option>
                        <option value="divorced">Divorced</option>
                    </select>
                </div>
            </div>
            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Profession">Profession</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <select class="form-control o_website_form_input" name="Profession" id="Profession" required="">
                        <option></option>
                        <option value="entrepreneur">Entrepreneur</option>
                        <option value="student">Student</option>
                        <option value="employee">Employee</option>
                        <option value="sector">Sector</option>
                        <option value="retired">Retired</option>
                    </select>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Spouses_name">Spouses name</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="Spouses_name" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Fathers_name">Fathers name</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="Fathers_name" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="document_type">Document type</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="document_type" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="reasons">Reasons</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <select class="form-control o_website_form_input" name="reasons" id="reasons" required="">
                        <option></option>
                        <option value="For_economic_interests">For economic interests</option>
                        <option value="for_professional_interests">For professional interests</option>
                        <option value="for_social_interests">For social interests</option>
                    </select>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="specify">Specify</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <select class="form-control o_website_form_input" name="specify" id="specify" required="">
                        <option></option>
                        <option value="company_creation">Company creation</option>
                        <option value="studies">Studies</option>
                        <option value="buy_hous">Buy hous</option>
                        <option value="open_bank_account">Open bank account</option>
                        <option value="other">Other</option>
                    </select>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="place_of_presentation">Place of presentation</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <select class="form-control o_website_form_input" name="place_of_presentation" id="place_of_presentation" required="">
                        <option></option>
                        <option value="immigration_Office">Immigration office</option>
                        <option value="police_station">Police station</option>
                        <option value="consular_office">Consular office</option>
                    </select>
                </div>
            </div>
            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="passport_number">Passport number</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="passport_number" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="delivered_by">Delivered by</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="delivered_by" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Passport_delivery_date">Passport delivery date</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="date" class="form-control o_website_form_input" name="Passport_delivery_date" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="Passport_expiration_date">Passport expiration date</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="date" class="form-control o_website_form_input" name="Passport_expiration_date" required=""/>
                </div>
            </div>

            <div class="form-group row form-field o_website_form_required_custom">
                <div class="col-lg-3 col-md-4">
                    <label class="col-form-label" for="foreigner_identity_number">Foreigner identity number</label>
                </div>
                <div class="col-lg-7 col-md-8">
                    <input type="text" class="form-control o_website_form_input" name="foreigner_identity_number" required=""/>
                </div>
            </div>

        </xpath>
       
    </template>
     
</odoo>
