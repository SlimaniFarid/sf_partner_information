<odoo>
  <data>

      <record model="ir.ui.view" id="personal_information_inherit">
        <field name="name">personal.information.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="partner_contact_personal_information_page.personal_information"/>
        <field name="arch" type="xml">
          <xpath expr="//page[@name='personal_information_page']" position="inside">
              <group>
                  <group>
                    <field name="Place_of_birth"/>  
                    <field name="family_situation"/>
                    <field name="Profession"/>    
                    <field name="Spouses_name"/>    
                    <field name="Fathers_name"/>  
                  </group>
                  <group>
                      <field name="document_type"/>   
                      <field name="reasons"/>    
                      <field name="specify"/>
                      <field name="place_of_presentation"/>
                      <field name="passport_number"/> 
                      <field name="delivered_by"/>     
                      <field name="Passport_delivery_date"/>  
                      <field name="Passport_expiration_date"/> 
                      <field name="foreigner_identity_number"/> 
                  </group>
              </group>
          </xpath>
        </field>
      </record>
         
  </data>
</odoo>